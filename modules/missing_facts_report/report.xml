							</if>
						</if>
					</Gedcom>
				</if>

				<!-- MARM -->
				<if condition="$fmarr=='1'">
					<SetVar name="nrevent" value="$nrevent + 2" />
					<Gedcom id="@FAMS">
						<if condition="@MARR:DATE==''">
							<SetVar name="nrmissingevent" value="$nrmissingevent + 1" />
							<if condition="$printname=='1'">
								<Cell style="list_value" width="$name"> </Cell>
							</if>
