<?xml version="1.0" encoding="UTF-8"?>
<PGVReport>
<!-- $Id$ -->
	<PGVRTitle><PGVRvar var="i18n::translate('Descendancy Report')" /></PGVRTitle>
	<PGVRDescription>Prints a descendant chart in portrait mode</PGVRDescription>
	<PGVRInput name="pid" lookup="INDI" type="text" default=""><PGVRvar var="i18n::translate('Enter Individual ID')" /></PGVRInput>
	<PGVRInput name="maxgen" type="select" options="3=>3|4=>4|5=>5|6=>6|7=>7|8=>8|9=>9|*=>*"><PGVRvar var="i18n::translate('Generations')" /></PGVRInput>
	<PGVRInput name="sources" type="checkbox" default="1"><PGVRvar var="i18n::translate('Show sources?')" /></PGVRInput>
	<PGVRInput name="pageSize" type="select" default="A4" options="letter=>i18n::translate_c('paper size','Letter')|A3=>i18n::translate_c('paper size', 'A3')|A4=>i18n::translate_c('paper size','A4')|legal=>i18n::translate_c('paper size','Legal')"><PGVRvar var="i18n::translate('Page size')" /></PGVRInput>
	<PGVRInput name="fonts" type="select" default="dejavusans" options="arialunicid0=>i18n::translate_c('font name', 'Arial')|dejavusans=>i18n::translate_c('font name', 'DejaVu')|helvetica=>i18n::translate_c('font name', 'Helvetica')"><PGVRvar var="i18n::translate('Font')"/></PGVRInput>

	<!-- Header -->
	<PGVRStyle name="header" font="$fonts" size="18"/>
	<!-- Page numbers text -->
	<PGVRStyle name="pagenum" font="$fonts" size="8"/>
	<!-- Standard text -->
	<PGVRStyle name="text" font="$fonts" size="9"/>
	<!-- Source text - PGVRFootnoteTexts - required style name by the generator -->
	<PGVRStyle name="footnote" font="$fonts" size="8"/>
	<!-- Links to sources - required style name by the generator -->
	<PGVRStyle name="footnotenum" font="$fonts" size="7"/>
	<!-- Generated by style - required style name by the generator -->
	<PGVRStyle name="genby" font="$fonts" size="8" />
	<!-- Date text -->
	<PGVRStyle name="date" font="$fonts" size="8"/>
	<PGVRStyle name="subheader" font="$fonts" size="12" style="b" />
	<PGVRStyle name="label1" font="$fonts" size="8"/>
	<PGVRStyle name="label2" font="$fonts" size="10"/>
	<PGVRStyle name="name" font="$fonts" size="9"/>
	<PGVRStyle name="name1" font="$fonts" size="14"/>
	
	<PGVRSetVar name="personNumber" value="1" />
	<PGVRSetVar name="childNumber" value="2" />
	<PGVRSetVar name="generation" value="0" />
	<!-- New Page for Sources controller if SOUR exist -->
	<PGVRSetVar name="source_exist" value="0" />
	
	<PGVRDoc pageSize="$pageSize">
		<PGVRHeader>
			<PGVRCell align="center" height="30" newline="1" style="header"><PGVRvar var="i18n::translate('Descendancy Report for ')" /><PGVRGetPersonName id="$pid" /></PGVRCell>
			<PGVRCell align="rightrtl" newline="1" style="pagenum"><PGVRvar var="i18n::translate('Page')" /> <PGVRPageNum /> <PGVRvar var="i18n::translate('of')" /> <PGVRTotalPages /></PGVRCell>
		</PGVRHeader>
		<PGVRBody>
			<PGVRRelatives id="$pid" group="descendants" sortby="generation" maxgen="$maxgen">
				<PGVRSetVar name="showPerson" value="F" />
				<PGVRRepeatTag tag="FAMS">
					<PGVRGedcom id="@FAMS:@CHIL">
						<PGVRSetVar name="showPerson" value="T" />
					</PGVRGedcom>
				</PGVRRepeatTag>
				<PGVRif condition="$showPerson=='T'">
					<PGVRif condition="$generation!=@generation">
						<PGVRCell align="center" height="20" newline="1" style="subheader"><PGVRvar var="i18n::translate('Generation ')" /> <PGVRGeneration /></PGVRCell>
						<PGVRSetVar name="generation" value="@generation" />
					</PGVRif>
					<PGVRTextBox border="" width="25" height="40">
						<PGVRText style="name"><PGVRvar var="personNumber" />.</PGVRText>
					</PGVRTextBox>

					<PGVRTextBox border="" height="40" newline="1">
						<PGVRText style="name"><PGVRGetPersonName id="" /> </PGVRText>
<!-- main -->			<PGVRif condition="$sources==1">
							<PGVRRepeatTag tag="SOUR">
								<PGVRFootnote>
									<PGVRGedcom id="@SOUR">
										<PGVRGedcomValue tag="TITL" />
									</PGVRGedcom>
									<PGVRif condition="@PAGE!=''">: </PGVRif>
									<PGVRGedcomValue tag="SOUR:PAGE"/> <PGVRGedcomValue tag="SOUR:DATA:TEXT"/>
								</PGVRFootnote>
								<PGVRSetVar name="source_exist" value="1"/>
							</PGVRRepeatTag>
						</PGVRif>
						<PGVRSetVar name="printPeriod" value="false" />
						<PGVRif condition="@SEX=='M'">
							<PGVRSetVar name="gender" value="male" />
						</PGVRif>
						<PGVRif condition="@SEX=='F'">
							<PGVRSetVar name="gender" value="female" />
						</PGVRif>
						<PGVRSetVar name="birtExists" value="false" />
						<PGVRGedcom id="BIRT">
							<PGVRSetVar name="birtExists" value="true" />	
							<PGVRSetVar name="printPeriod" value="true" />
							<PGVRif condition="$gender=='male'">
								<PGVRText style="label1"> <PGVRvar var="i18n::translate_c('MALE', 'was born')" /> </PGVRText>
							</PGVRif>
							<PGVRif condition="$gender=='female'">
								<PGVRText style="label1"> <PGVRvar var="i18n::translate_c('FEMALE', 'was born')" /> </PGVRText>
							</PGVRif>
							<PGVRif condition="$sources==1">
								<PGVRRepeatTag tag="BIRT:SOUR">
									<PGVRFootnote>
										<PGVRGedcom id="@SOUR">
											<PGVRGedcomValue tag="TITL" />
										</PGVRGedcom>
										<PGVRif condition="@PAGE!=''">: </PGVRif>
										<PGVRGedcomValue tag="PAGE" level="3"/> <PGVRGedcomValue tag="DATA:TEXT" level="3"/>
									</PGVRFootnote>
									<PGVRSetVar name="source_exist" value="1"/>
								</PGVRRepeatTag>
							</PGVRif>
							<PGVRif condition="@DATE!=''">
								<PGVRText style="label1"> <PGVRvar var=" " /> <PGVRGedcomValue tag="DATE" level="2" /></PGVRText>
							</PGVRif>
